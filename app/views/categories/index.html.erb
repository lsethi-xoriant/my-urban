<h1>Listing categories</h1>
<%= link_to 'New Category', new_category_path %></br>
<% if Category.exists? %>
<table id="categories">
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="page">

    <%= render @categories %>

  </tbody>

</table>

<%= paginate @categories %>
<% end %>




<br>
<%= link_to 'Back', events_path %>





